/*
for BT AT command
*/
//define 
#include "SoftwareSerial.h"
int led = 13;
char cmd;
SoftwareSerial BT(8,9);
//setup
void setup()
{
  pinMode(led, OUTPUT);
  Serial.begin(9600);
  BT.begin(9600);
}
//loop
void loop()
{
  if(Serial.available()>0){
    cmd = Serial.read();
    BT.print(cmd);
  }
  if(BT.available()>0){
    cmd = BT.read();
    Serial.print(cmd);
  }
}
